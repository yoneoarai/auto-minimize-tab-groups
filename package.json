  "license": "ISC",
  "devDependencies": {
    "@types/chrome": "^0.0.239",
    "@types/node": "^20.3.2",
    "ts-loader": "^9.5.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "copy-webpack-plugin": "^12.0.2"
  }
}
